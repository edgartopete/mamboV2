<div class="container">
  <div class="row">
    <form class="col s12">
      <div class="col s12 red">
        <h4>Store information</h4>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">account_circle</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Name</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">phone</i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">Telephone</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">location_city</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Street</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">location_on</i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">Number</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">location_on</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Zip Code</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">location_on</i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">State</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s6">
          <select>
            <option value="" disabled selected>Open hours</option>
            <option value="8">08:00</option>
            <option value="9">09:00</option>
            <option value="10">10:00</option>
            <option value="11">11:00</option>
            <option value="12">12:00</option>
            <option value="13">13:00</option>
          </select>
          <label>Store hours </label>
        </div>
        <div class="input-field col s6">
          <select>
            <option value="" disabled selected>Close hours</option>
            <option value="14">14:00</option>
            <option value="15">15:00</option>
            <option value="16">16:00</option>
            <option value="17">17:00</option>
            <option value="18">18:00</option>
            <option value="19">19:00</option>
            <option value="20">20:00</option>
          </select>

        </div>
      </div>
      
          <div class="row">
            <div class="input-field col s6">
              <input id="email" type="email" class="validate">
              <label for="email">Email</label>
              <span class="helper-text" data-error="wrong" data-success="right">Helper text</span>
            </div>
            <div class="input-field col s6">
          <input id="password" type="password" class="validate">
          <label for="password">Password</label>
        </div>
          </div>
       


      <div class="file-field input-field col s6">
        <div class="btn">
          <span>Logo</span>
          <input type="file">
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text" placeholder="Upload logo">
        </div>
      </div>
      <div class="file-field input-field col s6">
        <div class="btn">
          <span>Banner</span>
          <input type="file">
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text" placeholder="Upload banner">
        </div>
      </div>

      <div class="col s12 red">
        <h4>Contact information</h4>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">contacts</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Name</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">work</i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">Position</label>
        </div>
      </div>
    </form>
  </div>
</div>